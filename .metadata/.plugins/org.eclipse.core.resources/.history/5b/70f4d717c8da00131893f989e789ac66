package src.scoringsystem.network.packages;

import java.util.HashMap;

public class DataPackage
{
	
	public static HashMap<String, Object> baseDataMap = new HashMap<String, Object>();
	public static HashMap<String, Object> extraDataMap = new HashMap<String, Object>();
	
	
	//Logic
	public boolean addBaseObject(String Key, Object obj)
	{
		baseDataMap.put(Key, obj);
		return true;
	}
	
	//Getters
	
	public HashMap<String, Object> getBaseData()
	{
		return baseDataMap;
	}
	
	public HashMap<String, Object> getExtraData()
	{
		return extraDataMap;
	}
	
	//Setters
}
