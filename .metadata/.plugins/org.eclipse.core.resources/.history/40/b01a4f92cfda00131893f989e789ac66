package src.scoringsystem.database;

import java.util.Iterator;
import java.util.Set;

import src.scoringsystem.database.models.DataBaseModel;

public class Database
{
	public static void readFromDatabase()
	{
		
	}

	public static void writeToDatabase(DataBaseModel model)
    {
		Set<String> set = model.getModel().keySet();
		Iterator<String> it = set.iterator();
		while(it.hasNext())
		{
			String key = it.next();
			model.getModel().get(key);
		}
    }
}
